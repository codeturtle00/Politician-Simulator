<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.politicgame">

    <application
        android:name=".Application.PoliticGameApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/BlueTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".SummaryActivity"></activity>
        <activity android:name=".UserActivity.LoginActivity.LoggedInActivity" />
        <activity android:name=".GamesActivity.SpeechGame.SpeechInstructionActivity" />
        <activity android:name=".GamesActivity.StampGame.StampActivityWon" />
        <activity android:name=".GamesActivity.StampGame.StampActivityLost" />
        <activity android:name=".GamesActivity.SpeechGame.FailureSpeechResult" />
        <activity android:name=".GamesActivity.SpeechGame.SuccessSpeechResult" />
        <activity android:name=".GamesActivity.SpeechGame.SpeechResult" />
        <activity
            android:name=".LeaderBoardActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".GamesActivity.StampGame.StampActivity" />
        <activity android:name=".GamesActivity.StampGame.StampInstructionActivity" />
        <activity android:name=".GamesActivity.SpeechGame.SpeechActivity" />
        <activity android:name=".GamesActivity.BabyGame.BabyActivity" />
        <activity android:name=".GamesActivity.BabyGame.BabyGameInstruction" />
        <activity android:name=".UserActivity.LoginActivity.LoginActivity" />
        <activity android:name=".UserActivity.RegisterActivity.RegistrationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PauseActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".UserActivity.RegisterActivity.SelectCharacterActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".UserActivity.LoginActivity.LoadCharacterActivity" />
    </application>

</manifest>